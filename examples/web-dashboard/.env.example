# ANALYTICA Web Dashboard - Environment Variables
# ================================================
# Copy this file to .env and fill in your values

# API Configuration
ANALYTICA_API_URL=http://localhost:18000
ANALYTICA_API_KEY=your-api-key-here

# App Settings
APP_NAME=Analytica Dashboard
APP_ENV=development

# Auto-refresh interval (milliseconds)
REFRESH_INTERVAL=30000

# Feature flags
ENABLE_REAL_TIME=true
ENABLE_EXPORT=true
ENABLE_DARK_MODE=true

# Analytics (optional)
GA_TRACKING_ID=
SENTRY_DSN=
