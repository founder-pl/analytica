# ANALYTICA DSL Pipelines - Environment Variables
# ===============================================
# Copy this file to .env and fill in your values

# API Configuration
ANALYTICA_API_URL=http://localhost:18000
ANALYTICA_API_KEY=your-api-key-here

# Default Data Sources
DEFAULT_DATA_PATH=./data
CSV_DELIMITER=,
JSON_INDENT=2

# Pipeline Settings
PIPELINE_TIMEOUT=30000
MAX_ROWS=100000
CACHE_ENABLED=true

# Output Settings
OUTPUT_DIR=./output
DEFAULT_EXPORT_FORMAT=json
