{
  "name": "analytica-mobile",
  "version": "1.0.0",
  "description": "Mobile financial app built with Analytica DSL",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "build:ios": "cd ios && xcodebuild -workspace AnalyticaMobile.xcworkspace -scheme AnalyticaMobile -configuration Release",
    "build:android": "cd android && ./gradlew assembleRelease",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-chart-kit": "^6.12.0",
    "axios": "^1.6.0",
    "@react-native-async-storage/async-storage": "^1.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/runtime": "^7.23.0",
    "@types/react": "^18.2.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.0"
  },
  "keywords": ["analytica", "dsl", "mobile", "react-native", "finance"],
