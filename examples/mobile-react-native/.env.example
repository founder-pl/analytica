# ANALYTICA Mobile React Native - Environment Variables
# =====================================================
# Copy this file to .env and fill in your values

# API Configuration
ANALYTICA_API_URL=http://localhost:18000
ANALYTICA_API_KEY=your-api-key-here

# App Settings
APP_NAME=Analytica Mobile
APP_ENV=development

# Push Notifications
FCM_SENDER_ID=your-fcm-sender-id
APNS_KEY_ID=your-apns-key-id

# Offline Storage
MAX_OFFLINE_ITEMS=1000
SYNC_INTERVAL=60000

# Feature flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_OFFLINE_MODE=true

# Analytics
ANALYTICS_ENABLED=false
CRASHLYTICS_ENABLED=false
